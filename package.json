{
  "name": "@kriszu/ni",
  "version": "0.0.3",
  "packageManager": "pnpm@8.0.0",
  "description": "Use the right package manager",
  "author": "Kriszu <wangsizhu0504@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/wangsizhu0504/ni#readme",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cli",
    "kriszu"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "ni": "bin/ni.mjs",
    "nr": "bin/nr.mjs",
    "nu": "bin/nu.mjs",
    "nun": "bin/nun.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "npm run build & npm link",
    "build": "pnpm run clean && pnpm run compile",
    "clean": "rimraf ./dist",
    "compile": "unbuild",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublish:cli": "npm run build"
  },
  "devDependencies": {
    "@kriszu/eslint-config": "^1.0.11",
    "@posva/prompts": "^2.4.4",
    "@types/fs-extra": "^11.0.1",
    "@types/ini": "^1.3.31",
    "@types/node": "^18.16.16",
    "@types/which": "^3.0.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.42.0",
    "esno": "^0.16.3",
    "execa": "^7.1.1",
    "fast-glob": "^3.2.12",
    "find-up": "^6.3.0",
    "fs-extra": "^11.1.1",
    "fzf": "^0.5.2",
    "ini": "^4.1.1",
    "kleur": "^4.1.5",
    "rimraf": "^5.0.1",
    "terminal-link": "^3.0.0",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1",
    "vitest": "^0.32.0",
    "which": "^3.0.1"
  }
}
