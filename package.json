{
  "name": "@kriszu/wi",
  "version": "0.0.1",
  "packageManager": "pnpm@8.0.0",
  "description": "Use the right package manager",
  "author": "Kriszu <wangsizhu0504@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/wangsizhu0504/ki#readme",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangsizhu0504/ki.git"
  },
  "bugs": "https://github.com/wangsizhu0504/ki/issues",
  "keywords": [
    "cli",
    "kriszu"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "wi": "bin/wi.mjs",
    "wr": "bin/wr.mjs",
    "wu": "bin/wu.mjs",
    "wun": "bin/wun.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "npm run build & npm link",
    "build": "pnpm run clean && pnpm run compile",
    "clean": "rimraf ./dist",
    "compile": "unbuild",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublish:cli": "npm run build"
  },
  "devDependencies": {
    "@kriszu/eslint-config": "^1.0.11",
    "@types/ini": "^1.3.31",
    "@types/node": "^17.0.45",
    "@types/prompts": "^2.4.4",
    "bumpp": "^9.1.0",
    "eslint": "^8.40.0",
    "esno": "^0.16.3",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.12",
    "find-up": "^6.3.0",
    "ini": "^3.0.1",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.0",
    "terminal-link": "^3.0.0",
    "unbuild": "^1.2.1",
    "vitest": "^0.30.1",
    "which": "^2.0.2"
  }
}
